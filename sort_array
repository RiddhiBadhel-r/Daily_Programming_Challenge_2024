import java.util.Arrays;
 
public class Main
{
 
  // main method
  public static void main(String[] args)
  {
 
    // Input array
    int[] arr = {0,1,2,1,0,2,1,0};
    int size = arr.length;
 
    // inbuilt sort function to sort the array
    Arrays.sort(arr);
 
    //  sorted array
    for (int i = 0; i < size; i++) {
      System.out.print(arr[i] + " ");
    }
    System.out.println();
  }
}
